<mapml>
    <head>
        <title>Arctic Ocean Basemap MapML Tile Cache</title>
        <meta http-equiv="Content-Type" content="text/mapml"/>
        <meta charset="utf-8"/>
        <meta name="projection" content="APSTILE"/>
        <link rel="license" href="https://www.esri.com/legal/software-license"
            title="Sources: Esri, GEBCO, NOAA, National Geographic, DeLorme, HERE, Geonames.org, and other contributors"
        />
    </head>
    <body>
        <extent units="APSTILE">
            <input name="xmn" type="location" position="top-left" axis="x"  min="0.0" max="2048.0"/>
            <input name="ymn" type="location"  position="top-left" axis="y" min="0.0" max="2048.0"/>
            <input name="xmx" type="location" position="bottom-right" min="0.0" max="2048.0"/>
            <input name="ymx" type="location"  position="bottom-right" min="0.0" max="2048.0"/>
            <input name="z" type="zoom" value="3" min="0" max="10"/>
            <input name="y" type="location" units="tilematrix" axis="row"/>
            <input name="x" type="location" units="tilematrix" axis="column"/>
            <link rel="tile" tref="https://server.arcgisonline.com/arcgis/rest/services/Polar/Arctic_Ocean_Base/MapServer/tile/{z}/{y}/{x}?m4h=t"/>
        </extent>
    </body>
</mapml>
