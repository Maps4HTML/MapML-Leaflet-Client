      <input name="ymin" type="location" units="pcrs" position="bottom-left" axis="northing"/>
      <input name="xmax" type="location" units="pcrs" position="top-right" axis="easting"/>
      <input name="ymax" type="location" units="pcrs" position="top-left" axis="northing"/>
      <input name="w" type="width"/>
      <input name="h" type="height"/>
      <template type="image" tref="http://maps.geogratis.gc.ca/wms/canvec_en?version=1.3.0&amp;service=WMS&amp;request=GetMap&amp;crs=EPSG:3978&amp;layers=land,hydro,man_made,resource_management,transport,administrative,elevation,toponymy&amp;styles=,,,,,,,&amp;bbox={xmin},{ymin},{xmax},{ymax}&amp;format=image/png&amp;transparent=false&amp;width={w}&amp;height={h}&amp;m4h=t"/>
