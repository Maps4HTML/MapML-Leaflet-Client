  <body>
    <extent units="CBMTILE" method="GET">
      <input name="xmn" type="xmin" min="78848.0" max="295424.0"/>
      <input name="ymn" type="ymin" min="103936.0" max="320512.0"/>
      <input name="xmx" type="xmax" min="78848.0" max="295424.0"/>
      <input name="ymx" type="ymax" min="103936.0" max="320512.0"/>
      <input name="z" type="zoom" value="10" min="10" max="15"/>
