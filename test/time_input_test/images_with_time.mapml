<mapml>
  <head>
    <title>Environment and Climate Change Canada - GDPS.DIAG - Dominant precipitation type</title>
    <meta http-equiv="Content-Type" content="text/mapml;projection=OSMTILE"/>
    <meta charset="utf-8"/>
    <meta name="projection" content="OSMTILE"/>
    <meta name="zoom" content="0"/>
    <base href="/mapml/en/osmtile/test/"/>
    <link rel="license" href="http://open.canada.ca/en/open-government-licence-canada" title="Open Government Licence - Canada"/>
    <link rel="legend" href="http://geo.weather.gc.ca/geomet//?LANG=E%26SERVICE=WMS%26VERSION=1.1.1%26REQUEST=GetLegendGraphic%26STYLE=PRECIPNW%26LAYER=GDPS.DIAG_NW%26format=image/png"/>
  </head>
  <body>
    <extent units="OSMTILE" method="GET">
      <input name="xmn" type="location" position="top-left" axis="x" min="0.0" max="256.0"/>
      <input name="ymn" type="location" position="top-left" axis="y" min="0.0" max="256.0"/>
      <input name="xmx" type="location" position="bottom-right" axis="x" min="0.0" max="256.0"/>
      <input name="ymx" type="location" position="bottom-right" axis="y" min="0.0" max="256.0"/>
      <input name="z" type="zoom" value="0" min="0" max="18"/>
      <input name="xmin" type="location" units="pcrs" position="top-left" axis="easting"/>
      <input name="ymin" type="location" units="pcrs" position="bottom-left" axis="northing"/>
      <input name="xmax" type="location" units="pcrs" position="top-right" axis="easting"/>
      <input name="ymax" type="location" units="pcrs" position="top-left" axis="northing"/>
      <input name="w" type="width"/>
      <input name="h" type="height"/>
      <input name="utc" type="datetime" min="2018-03-20T00:00:00Z" max="2018-03-30T00:00:00Z" value="2018-03-30T12:00:00Z" step="PT1H"/>
      <link rel="image" tref="http://geo.weather.gc.ca/geomet/?lang=E&amp;SERVICE=WMS&amp;VERSION=1.1.1&amp;SRS=EPSG:3857&amp;LAYERS=GDPS.DIAG_NW&amp;time={utc}&amp;&amp;BBOX={xmin},{ymin},{xmax},{ymax}&amp;REQUEST=GetMap&amp;FORMAT=image/png&amp;TRANSPARENT=TRUE&amp;WIDTH={w}&amp;HEIGHT={h}&amp;STYLES=PRECIPNW&amp;m4h=t"/>
      <input name="i" type="location" units="map" axis="i"/>
      <input name="j" type="location" units="map" axis="j"/>
      <link rel="query" tref="http://geo.weather.gc.ca/geomet/?lang=E&amp;SERVICE=WMS&amp;VERSION=1.1.1&amp;SRS=EPSG:3857&amp;LAYERS=GDPS.DIAG_NW&amp;time={utc}&amp;QUERY_LAYERS=GDPS.DIAG_NW&amp;time={utc}&amp;X={i}&amp;Y={j}&amp;INFO_FORMAT=text/plain&amp;BBOX={xmin},{ymin},{xmax},{ymax}&amp;REQUEST=GetFeatureInfo&amp;WIDTH={w}&amp;HEIGHT={h}&amp;m4h=t"/>
    </extent>
  </body>
</mapml>

